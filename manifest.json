{
  "manifest_version": 3,
  "name": "CheetSheet",
  "version": "1.0",
  "description": "A chrome extension helps you to build your sheets",
  "permissions": ["storage", "identity"],
  "oauth2": {
    "client_id": "633656739948-gisc279mm047abupnaau3mmgloc2ft9c.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/drive.readonly",
      "https://www.googleapis.com/auth/spreadsheets",
      "https://www.googleapis.com/auth/drive.metadata.readonly"
    ]
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icons/cheet.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
